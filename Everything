              }

              .accepted {
                  color: #008000;
              }
          </style>
      </head>
      <body>
          <header>
              <h1>Application Acceptance System</h1>
          </header>

          <main>
              <form id="myForm">
                  <label for="name">Name:</label>
                  <input type="text" id="name" required>

                  <label for="age">Age:</label>
                  <input type="number" id="age" required>

                  <button type="button" onclick="acceptApplication()">Accept</button>
              </form>

              <div id="result"></div>
          </main>

          <script>
              function acceptApplication() {
                  // Get values from form
                  var name = document.getElementById('name').value;
                  var age = document.getElementById('age').value;

                  // Display result with accepted status
                  var resultElement = document.getElementById('result');
                  resultElement.innerHTML = 'Application accepted!<br>';
                  resultElement.innerHTML += 'Name: ' + name + '<br>';
                  resultElement.innerHTML += 'Age: ' + age;
                  resultElement.className = 'accepted';
              }
          </script>
      </body>
      </html>


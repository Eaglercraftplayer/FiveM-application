      <body>
          <header>
              <h1>Application Pending System</h1>
          </header>

          <main>
              <form id="myForm">
                  <label for="name">Name:</label>
                  <input type="text" id="name" required>

                  <label for="age">Age:</label>
                  <input type="number" id="age" required>

                  <button type="button" onclick="markApplicationPending()">Mark as Pending</button>
              </form>

              <div id="result"></div>
          </main>

          <script>
              function markApplicationPending() {
                  // Get values from form
                  var name = document.getElementById('name').value;
                  var age = document.getElementById('age').value;

                  // Display result with pending status
                  var resultElement = document.getElementById('result');
                  resultElement.innerHTML = 'Application marked as pending!<br>';
                  resultElement.innerHTML += 'Name: ' + name + '<br>';
                  resultElement.innerHTML += 'Age: ' + age;
                  resultElement.className = 'pending';
              }
          </script>
      </body>

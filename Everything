          </header>

          <main>
              <form id="myForm">
                  <label for="name">Name:</label>
                  <input type="text" id="name" required>

                  <label for="age">Age:</label>
                  <input type="number" id="age" required>

                  <button type="button" onclick="denyApplication()">Deny</button>
              </form>

              <div id="result"></div>
          </main>

          <script>
              function denyApplication() {
                  // Get values from form
                  var name = document.getElementById('name').value;
                  var age = document.getElementById('age').value;

                  // Display result with denied status
                  var resultElement = document.getElementById('result');
                  resultElement.innerHTML = 'Application denied!<br>';
                  resultElement.innerHTML += 'Name: ' + name + '<br>';
                  resultElement.innerHTML += 'Age: ' + age;
                  resultElement.className = 'denied';
              }
          </script>
      </body>
